{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
      "next-commerce#build": {
         "dependsOn": [
            "^build",
            "$COMMERCE_PROVIDER",
            "$NEXT_PUBLIC_VENDURE_SHOP_API_URL",
            "$VENDURE_CHANNEL_REST_URL",
            "$NEXT_PUBLIC_VENDURE_CHANNEL_TOKEN",
            "$IMAGE_DOMAINS"
         ],
         "outputs": [
            ".next/**"
         ]
      },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
