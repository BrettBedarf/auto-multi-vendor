{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
      "next-commerce#build": {
         "dependsOn": [
            "^build",
            "$COMMERCE_PROVIDER",
            "$BIGCOMMERCE_STOREFRONT_API_URL",
            "$NEXT_PUBLIC_SHOPIFY_STORE_DOMAIN",
            "$NEXT_PUBLIC_SWELL_STORE_ID"
         ],
         "outputs": [
            ".next/**"
         ]
      },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
